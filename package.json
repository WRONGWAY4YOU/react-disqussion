{
  "name": "react-disqussion",
  "version": "1.0.4",
  "description": "Disqus library for React",
  "author": "Dmitry Guzeev <dmitry.guzeev@yahoo.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wrongway4you/react-disqussion.git"
  },
  "keywords": [
    "react",
    "disqus"
  ],
  "homepage": "https://github.com/wrongway4you/react-disqussion",
  "npmName": "react-disqussion",
  "browser": "build/dist/index.js",
  "main": "build/lib/index.js",
  "module": "build/es/index.js",
  "jsnext:main": "build/es/index.js",
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "pretest": "yarn run lint:src",
    "prepublish": "yarn run clean && yarn run lint && yarn run build",
    "test:watch": "cross-env BUILD_ENV=test karma start tools/karma.conf.js",
    "test:single": "cross-env BUILD_ENV=test karma start tools/karma.conf.js --single-run",
    "build:commonjs": "babel-node tools/build commonjs",
    "build:es": "babel-node tools/build es",
    "build:umd": "babel-node tools/build umd",
    "build:umd:min": "babel-node tools/build umd-min",
    "prebuild": "yarn run build:commonjs && yarn run build:es",
    "build": "yarn run build:umd && yarn run build:umd:min",
    "lint:src": "eslint src",
    "lint:examples": "eslint examples",
    "lint": "yarn run lint:src && yarn run lint:examples",
    "docs:gen": "babel-node tools/documentation-manager gen",
    "docs:clean": "babel-node tools/documentation-manager clean",
    "clean": "babel-node tools/clean"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.5.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "cross-env": "^5.0.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.8.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-react": "^7.0.1",
    "jsdoc": "^3.4.3",
    "jsdoc-to-markdown": "^3.0.0",
    "json-loader": "^0.5.4",
    "karma": "^1.7.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-cli": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-nyan-reporter": "^0.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.4.2",
    "mocha-loader": "^1.1.1",
    "phantomjs-prebuilt": "^2.1.14",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "readdir-recursive": "^0.0.4",
    "rimraf": "^2.6.1",
    "webpack": "^2.6.1"
  }
}
