/* The configuration file for JSDoc utility */

{
  "source": {
    "include": ["src", "tools"],
    "exclude": ["node_modules"],
    "includePattern": ".+\\.jsx?",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "encoding": "utf8",
    "destination": "docs/documentation",
    "recurse": true,
    "readme": "README.md"
  },
  "plugins": []
}
